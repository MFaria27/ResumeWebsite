		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tsconfig/svelte": "^5.0.2",
		"gh-pages": "^6.1.1",
		"svelte": "^4.2.7",
		"vite": "^5.2.8"
	},
	"type": "module",
	"dependencies": {
		"bulma": "^0.9.4",
		"lodash": "^4.17.21",
		"node-sass": "^9.0.0",
		"particles.js": "^2.0.0",
		"sass": "^1.70.0",
		"svelte-check": "^3.6.3"
